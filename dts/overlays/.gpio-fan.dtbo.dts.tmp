# 1 "arch/arm/boot/dts/overlays/gpio-fan-overlay.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm/boot/dts/overlays/gpio-fan-overlay.dts"
# 37 "arch/arm/boot/dts/overlays/gpio-fan-overlay.dts"
/dts-v1/;
/plugin/;

/ {
 compatible = "brcm,bcm2835";

 fragment@0 {
  target-path = "/";
  __overlay__ {
   fan0: gpio-fan@0 {
    compatible = "gpio-fan";
    gpios = <&gpio 12 0>;
    gpio-fan,speed-map = <0 0>,
          <5000 1>;
    #cooling-cells = <2>;
   };
  };
 };

 fragment@1 {
  target = <&cpu_thermal>;
  polling-delay = <2000>;
  __overlay__ {
   trips {
    cpu_hot: trip-point@0 {
     temperature = <55000>;
     hysteresis = <10000>;
     type = "active";
    };
   };
   cooling-maps {
    map0 {
     trip = <&cpu_hot>;
     cooling-device = <&fan0 1 1>;
    };
   };
  };
 };
 __overrides__ {
  gpiopin = <&fan0>,"gpios:4", <&fan0>,"brcm,pins:0";
  temp = <&cpu_hot>,"temperature:0";
 };
};
