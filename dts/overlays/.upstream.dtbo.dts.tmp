# 1 "<command-line>"
# 1 "arch/arm/boot/dts/overlays/upstream-overlay.dts"


/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/clock/bcm2835.h" 1
