# 1 "arch/arm/boot/dts/overlays/bmp085_i2c-sensor-overlay.dts"
# 1 "<command-line>"
# 1 "arch/arm/boot/dts/overlays/bmp085_i2c-sensor-overlay.dts"

/dts-v1/;
/plugin/;

/ {
        compatible = "brcm,bcm2835";

        fragment@0 {
                target = <&i2c_arm>;
                __overlay__ {
                        #address-cells = <1>;
                        #size-cells = <0>;
                        status = "okay";

                        bmp085@77 {
                                compatible = "bosch,bmp085";
                                reg = <0x77>;
                                default-oversampling = <3>;
                                status = "okay";
                        };
                };
        };
};
