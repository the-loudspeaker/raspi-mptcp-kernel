# 1 "arch/arm/boot/dts/overlays/spi4-1cs-overlay.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm/boot/dts/overlays/spi4-1cs-overlay.dts"
/dts-v1/;
/plugin/;


/ {
 compatible = "brcm,bcm2711";

 fragment@0 {
  target = <&spi4_cs_pins>;
  frag0: __overlay__ {
   brcm,pins = <4>;
   brcm,function = <1>;
  };
 };

 fragment@1 {
  target = <&spi4>;
  frag1: __overlay__ {

   #address-cells = <1>;
   #size-cells = <0>;

   pinctrl-names = "default";
          pinctrl-0 = <&spi4_pins &spi4_cs_pins>;
   cs-gpios = <&gpio 4 1>;
   status = "okay";

   spidev4_0: spidev@0 {
    compatible = "spidev";
    reg = <0>;
    #address-cells = <1>;
    #size-cells = <0>;
    spi-max-frequency = <125000000>;
    status = "okay";
   };
  };
 };

 __overrides__ {
  cs0_pin = <&frag0>,"brcm,pins:0",
      <&frag1>,"cs-gpios:4";
  cs0_spidev = <&spidev4_0>,"status";
 };
};
