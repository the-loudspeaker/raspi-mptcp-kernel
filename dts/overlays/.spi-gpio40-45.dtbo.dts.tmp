# 1 "arch/arm/boot/dts/overlays/spi-gpio40-45-overlay.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm/boot/dts/overlays/spi-gpio40-45-overlay.dts"




/dts-v1/;
/plugin/;

/ {
 compatible = "brcm,bcm2835";

 fragment@0 {
  target = <&spi0>;
  __overlay__ {
   cs-gpios = <&gpio 43 1>, <&gpio 44 1>, <&gpio 45 1>;
   status = "okay";
  };
 };

 fragment@1 {
  target = <&spi0_cs_pins>;
  __overlay__ {
   brcm,pins = <45 44 43>;
   brcm,function = <1>;
   status = "okay";
  };
 };

 fragment@2 {
  target = <&spi0_pins>;
  __overlay__ {
   brcm,pins = <40 41 42>;
   brcm,function = <3>;
   status = "okay";
  };
 };
};
