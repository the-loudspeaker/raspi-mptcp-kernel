# 1 "arch/arm/boot/dts/overlays/sdhost-overlay.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm/boot/dts/overlays/sdhost-overlay.dts"
/dts-v1/;
/plugin/;



/{
 compatible = "brcm,bcm2835";

 fragment@0 {
  target = <&sdhost>;
  frag0: __overlay__ {
   brcm,overclock-50 = <0>;
   brcm,pio-limit = <1>;
   status = "okay";
  };
 };

 fragment@1 {
  target = <&mmc>;
  __overlay__ {
   status = "disabled";
  };
 };

 fragment@2 {
  target = <&mmcnr>;
  __overlay__ {
   status = "disabled";
  };
 };

 __overrides__ {
  overclock_50 = <&frag0>,"brcm,overclock-50:0";
  force_pio = <&frag0>,"brcm,force-pio?";
  pio_limit = <&frag0>,"brcm,pio-limit:0";
  debug = <&frag0>,"brcm,debug?";
 };
};
