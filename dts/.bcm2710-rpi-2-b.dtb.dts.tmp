 clock-frequency = <100000>;
};

&i2c2 {
 clock-frequency = <100000>;
};

&i2s {
 pinctrl-names = "default";
 pinctrl-0 = <&i2s_pins>;
};

&leds {
 act_led: act {
  label = "led0";
  linux,default-trigger = "mmc0";
  gpios = <&gpio 47 0>;
 };

 pwr_led: pwr {
  label = "led1";
  linux,default-trigger = "input";
  gpios = <&gpio 35 0>;
 };
};

&hdmi {
 hpd-gpios = <&gpio 46 1>;
};

&audio {
 pinctrl-names = "default";
 pinctrl-0 = <&audio_pins>;
};

